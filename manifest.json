{
  "name": "PXP Time Tracker",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "PXP Timer",
  "icons": {
    "16": "icons/icon_036.png",
    "48": "icons/icon_048.png",
    "128": "icons/icon_072.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "browser_action":{
    "default_popup" : "src/popup/popup.html"
  },
  "permissions": ["http://djangoreddit.me", "cookies"]
} 